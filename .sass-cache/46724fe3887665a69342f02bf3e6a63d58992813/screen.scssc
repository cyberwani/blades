/* ============================================= */
/*	Homepage						   			 */ 
/* ============================================= */


.cover,
.cover-single {
	position: relative;
	width: 100%;
	overflow: hidden;
	height: 350px;
	border-bottom: 1px solid #222;

	@media (min-width: 800px) {
		height: 500px;
	}
}

.cover-deep {
	@extend .cover-single;

	@media (min-width: 600px) {
		height: 500px;
	}

	@media (min-width: 800px) {
		height: 700px;
	}

	.slide {
		height: 700px;
	}
}

.cover-credit {
	font-size: 10px;
	padding: 5px;
	position: absolute;
	top: 5px;
	right: 5px;
	@include border-radius(2px);
	background-color: rgba(0,0,0,.5);
	color: rgba(255,255,255,.5);
	@include trans;
}

.slide {
	@include background-size(cover);
	background-repeat: no-repeat;
	float: left;
	width:100%;
	height: 500px;

	@media (max-width: 800px) {
		background-position: 50% 50%;
	}

	@media (min-width: 800px) {
		height: 500px;
	}
}


/* ============================================= */
/*	Universal styles					   		 */ 
/* ============================================= */

.resp-img {
	max-width: 100%;
}

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

body {
	background-color: $bg-black;
	color: $text;
}


/* ============================================= */
/*	Type styles					   				 */ 
/* ============================================= */

.txt-lg {
	font: 300 17px/185% $proxima;
	@media (min-width: 500px) {
		font-size: 19px;
	}
	> p {
		margin-bottom: 18px;		
	}
}

.txt {
	font: 100 16px/150% $proxima;
	margin: 0 0 20px 0;
	> p {
		margin-bottom: 18px;		
	}
}

.txt-sm {
	font: 300 14px/150% $proxima;
	> p {
		margin-bottom: 18px;		
	}
}

.txt a, .txt-lg a{
	color: #666;

	&:hover {
		color: $highlight-hover;
	}
}

.txt-dark {
	color: #666;
	font-weight: 400;
}

.txt-center {
	text-align: center;
}

.h1, .h2, .h3 {
	font-family: $adelle;
	font-weight: 100;
}

.h4 {
	font-family: $proxima;
	font-weight: 700;
	text-transform: uppercase;
	letter-spacing: 3px;
	color: #fff;
	margin-bottom: 20px;
}

.h1 {
	font-size: 34px;
	@media (min-width: 500px) {	
		font-size: 42px;  	
	} 
}
.h2 {	
	font-size: 30px;
	margin-bottom: 20px;
	@media (min-width: 500px) {	
		font-size: 36px;  	
	} 
}
.h3 {	font-size: 23px; margin-bottom: 10px;  }
.h4 {	font-size: 14px;  }
.h5 {	font-size: 16px;  }
.h6 {	font-size: 12px;  }


.tile-h4 {
	margin: 10px 0 0 0;
	color: #999;
	font: 600 14px/17px $proxima;
	@include trans;
}

.section-h {
	@extend .h4;
	margin: 50px auto 30px;
	text-align: center;
	.inset & {
		margin-top: 20px;
	}
}

.article-h1 {
	@extend .h1;
	padding: 30px 0 0 0;
	margin: 0 0 20px 0;
	color: #000;
	text-align: center;
	@media (min-width: 500px) {
		font-size: 52px;
	}
}

.kicker {
	font: 900 18px $proxima;
	text-align: center;
	position: relative;
	padding-bottom: 12px;
	margin: 0 auto 40px;
	&:after {
		@include content;
		height: 0;
		width: 200px;
		margin: auto;
		border-top: 1px solid #aaa;
		@include absolute(n 0 0 0); 
	}
}

.dropcap {
	font: 100 61px/104% $proxima;
	display: block;
	float: left;
	margin-right: 8px;
	margin-bottom: -6px;
	@media (min-width: 500px) {
		font-size: 72px;
	}
}

.txt-defaults {
	.attr {
		font-size: 11px;
		color: #999;
		letter-spacing: 2px;
		text-transform: uppercase;
		margin-bottom: 0;
	}

	a {
		color: $link;
	}

	ul {
		list-style-type: disc;
	}

	ol {
		list-style-type: decimal;
	}

	ul > li,
	ol > li {
		list-style-position: outside;
		margin-left: 20px;
		margin-bottom: 10px;
	}

	// spacing for heading elements 
	h2, h3, h4, h5, h6 {
		margin: 65px 0 10px 0;
	}

	// Subheaders inside sections
	// Follows .h2 style. Not sure why. (-tito)
	.h3, h3 {
		font: 100 30px $adelle;
		margin-bottom: 16px;
		@media (min-width: 500px) {
			font-size: 36px;
		}
	}

	// All other headers
	h4, h5, h6 {
		font: 600 18px/133% $proxima;
	}

	img {
		display: block;
		margin: auto;		
	}

	// When images are inserted directly into post
	// make sure they have some margin
	> img {
		margin: 20px 0;
	}

	//Use these to style images and stuff in blog posts
	.img-border {
		padding: 20px 0;
		border-bottom: 1px dotted #CCC;
		border-top: 1px dotted #CCC;
	}

	.caption {
		font: 600 14px/16px $proxima;
	}

	.caption.right,
	.alignright .caption {
		text-align: right;
	}

	.alignnone {
		width: 100%;
		height: auto;
		margin: 20px 0;

		img {
			margin: 20px 0;
			width: 100%;
			height: auto;
		}
	}

	.alignright {
		@media (min-width: 600px) {
			float: right;
			margin: 0 0 20px 40px;
		}		
	}
} // txt-defaults

.video-embed {
	@include video;
}



/* ============================================= */
/*	Layout Boxes						   		 */ 
/* ============================================= */

.txt-width {
	@media (min-width: 525px){
		max-width: 525px;
		margin: auto;
	}	
}

// For containing long runs of txt
// Can be used in conjunction with article-txt, txt, txt-lg, etc.
.l-txt {
	@extend .txt-defaults;
	@extend .txt-width;
}

// Site-wide containment element ‚Äî sets max-width,
// appropriate margins on mobile, tablet, etc.
.wrapper,
.layout,
.site-width {
	padding: 0 30px;

	@media (min-width: 600px) {
		padding: 0 40px;
	}

	@media (min-width: 900px){
		max-width: 1100px;
		margin: 0 auto;
	}
}

#content {
	padding: 0 0 0 0;
}

//Three side by side blocks, infinite number
// [ 1 ] [ 2 ] [ 3 ]
// [ 4 ] [ 5 ]  ...

.grid-container-3 {
	margin: 40px 0;
	@media(min-width: 500px) and (max-width: 699px){
		@include media-grid(2, 30px);
	}
	@media(min-width: 750px){
		@include media-grid(3, 60px);
	}

	.media-block{
		margin-bottom: 40px;
	}
}


/* ============================================= */
/*	Object Boxes						   		 */ 
/* ============================================= */

// Used for content striping on black backgrounds 
.inset {
	background-color: #161616;
	border-top: 1px solid #222;
	border-bottom: 1px solid #222;
	padding: 30px 0;
}

// Used for content striping on white backgrounds
.inset-light {
	background-color: #f1f1f1;
	padding: 60px 0;
}

.jagged-stage {
	border: 1px solid #ccc;
	border-bottom: 0;
	background-color: #fff;
	position: relative;
	margin-bottom: 100px;
	max-width: 700px;
	padding: 3% 3% 60px;
	@media (min-width: 760px) {
		margin-left: auto;
		margin-right: auto;
		padding: 30px 30px 60px;
	}
	&:after {
		@include content;
		height: 26px;
		background: transparent image-url('page-jag.png') repeat-x 50% 50%;
		@include absolute(n 0 -13px 0);
	}
}

.pin-bubble {
	color: #fff;
	@include bg(black, .8);
	padding: 12px;
	text-align: center;
	margin-top: 20px;
	&:before {
		@include content;
	}
	@media (max-width: 767px) {
		&:before {
			position: absolute;
			margin: -22px auto 0;
			@include triangle(top, 10px, rgba(0,0,0,.8), .9);
		}
	} //mq
	@media (min-width: 768px) {
		display: block;
		height: 150px;
		width: 150px;
		padding: 30px;
		margin-top: 0;
		z-index: 1;
		@include border-radius(100px);
		&.left {
			@include absolute(n n n -50px);
			&:before {
				@include triangle(right, 10px, rgba(0,0,0,.8), .9);
				@include absolute(69px -9px n n);
			}
		}
		&.right {
			@include absolute(n -50px n n n);
			&:before {
				@include triangle(left, 10px, rgba(0,0,0,.8), .9);
				@include absolute(69px n n -9px);
			}
		}
	} // mq
}


/* ============================================= */
/*	Header						   			 */ 
/* ============================================= */


.hdr {
	background-color: #000;
	padding: 15px 0 0px 0;
	margin: 0 0 0px 0;
	border-bottom: 1px solid #1d1d1d;

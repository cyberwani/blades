		}

		.image-count-2 {
			margin: 20px -150px;
			@include pie-clearfix;

			img {
				width: 50%;
				float: left;
			}
		}

		.image-count-3 {
			margin: 20px -150px;
			@include pie-clearfix;
			
			img {
				width: 33%;
				float: left;
			}
		}
	}//media > 900
}

.portfolio-desc {
	clear: both;
}


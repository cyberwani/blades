/* ============================================= */
/*	Tiles						   			 */ 
/* ============================================= */


#archive-tiles {
	padding-top: 30px;
	@include trans;

	@media (min-width: 751px){
		@include media-grid(4, 30px);
		.media-block {
			margin-bottom: 20px;
		}
	}

	@media (min-width: 500px) and (max-width: 750px){
		@include media-grid(3);
		margin-bottom: 20px;
	}

	@media (max-width: 499px){
		@include media-grid(2);
		margin-bottom: 20px;
	}
}

.tile {
	position: relative;
	margin-bottom: 20px;

	@media(min-width: 768px){
		/* opacity: .5; */
		@include trans;

		.wrapper:hover &{
			opacity: 1;
		}
	}

	html.touch &{
		opacity: 1;
	}
}

.tile img {
	@include border-radius(5px 5px 0 0);
	width: 100%;
	border: 10px solid #1d1d1d;
	@include trans;
}

.tile-h3 {
	padding: 10px 0;
	height: 60px;
	text-align: center;
	color: #666;
	font: 400 18px/22px $proxima;
	@include trans;

	.tile:hover & {
		color: #FFF;
		opacity: 1;
	}
}

/* ============================================= */
/*	Blog archive					   			 */ 
/* ============================================= */

#archive-blog, .hp-blog-list {
	.h1 {
		color: #1d1d1d;
	}

	.archive-blog-h1 {
		@extend .h1;
		color: #666;
		padding: 10px 0;
	}

	.h3 {
		color: #CCC;
	}

	.txt, .txt-oversize {
		@extend .txt-dark;
	}

	.read-more {
		@extend .txt;
		color: #444;

		&:hover {
			color: $highlight-hover;
		}
	}
}

.blog-small {
	.txt {
		margin-bottom: 40px;
	}
}

#archive-blog .blog-small, 
.hp-blog-list .blog-small  {
	.h3:hover,
	.read-more:hover { 
		color: #fff; 
	}
}

.article-hgroup {
	margin-bottom: 50px;
}

.blog-author {
	font: 600 14px/20px $proxima;
	margin: 10px 0;
	text-transform: uppercase;
	letter-spacing: 1px;
	color: #444;

	span {
		color: #666;
		font-family: $serif;
		font-style: italic;
		text-transform: capitalize;
		font-weight: normal;
	}
}

#archive-blog-featured {
	// margin: 30px 0 30px 0;
	.read-more {
		color: $highlight;
	}

	@media (min-width: 900px){
		@include media-grid(2, 60px);
	}
}

.featured-wrapper {
	background-color: #FFF;
}

.blog-featured {
	margin-bottom: 60px;
}

.blog-feat-img {
	display: block;
	float: left;
	width: 100px;
	height: 100px;
	margin: 10px 20px 10px 0;
	overflow: hidden;
	@include box-shadow(0px 0px 10px #CCC);
	@include border-radius(100px);

	@media (min-width: 900px){
		float: right;
		width: 170px;
		height: 170px;
		margin-left: 20px;
		margin-right: 0;
	}

	img {
		width: 100%;
		height: auto;
	}
}

#archive-blog-cron {
	padding-top: 20px;

	@media (min-width: 600px) and (max-width: 959px){
		@include media-grid(2);
	}

	@media (min-width: 960px){
		@include media-grid(4, 60px);
	}

	.blog {
		margin: 0 0 30px 0;
	}
}

//Quick and shitty IE8 media-grid fix:
html.ie8 #archive-blog-cron .media-block {
		@media (min-width: 600px) and (max-width: 959px){
			height: 225px;
		}
		@media (min-width: 960px) {
			height: 400px;
		}
}

.blog-load-more {
	display: block;
	width: 80px;
	height: 80px;
	padding: 24px 20px 20px;
	margin: 0 auto 0;
	text-align: center;
	@include border-radius(100px);
	background-color: #222;
	color: #fff;
	font-family: $proxima;
	@media (min-width: 600px) {
		margin-top: -20px;
	}
	.arrow {
		position: relative;
		top: -2px;
		@include svg(arrow-down);
		display: block;
		height: 25px;
		width: 30px;
		margin: auto;
		@include trans;
	}
	&:hover .arrow {
		top: 4px;
	}
	.more-txt {
		text-transform: uppercase;
		font-size: 11px;
		letter-spacing: 1px;
	}
	.more-txt,
	.arrow {
		opacity: .65;
	}
}

.archive-blog-ftr {
	padding: 60px 20px 20px;

	.xoxo {

		@media (min-width: 500px) and (max-width: 959px){
			@include media-grid(2, 20px);
		}

		@media (min-width: 960px){
			@include media-grid(4, 60px);
		}

		.widget-container {
			@extend .media-block;
			margin-bottom: 20px;

			.widget-title {
				@extend .h4;
			}

			ul li {
				@include inline-block;
				// margin: 0 3px 3px 0;
				a {
					padding: 5px;
					background-color: #000;
					display: block;
					color: #666;

					&:hover {
						color: $highlight;
					}
				}
			}
		}
	}//.xoxo
}


#searchform {
	background-color: #000;
	border: 1px solid #1d1d1d;
	@include border-radius(8px);
	padding: 10px;
	input {
		margin-top: 10px;
	}
	input[type="text"] {
		width: 100%;
	}
}

/* ============================================= */
/*	Blog detail 			 					 */ 
/* ============================================= */

.content-wrapper {
	background-color: #FFF;
	padding: 20px 0;

	@media (min-width: 900px){
		padding: 30px 0;
		margin: 0 auto;

		&.windowed {
			margin-top: -200px;
		}
	}

	.wrapper {
		max-width: 780px;
		margin: auto;
	}
}

//grouped these together as they share a lot of text styles
.article-txt, 
.portfolio-content {
	color: #333;
	margin-bottom: 20px;
	padding-bottom: 30px;

	p, ul, li, blockquote {
		font: 400 17px/28px $proxima;
		margin-bottom: 20px;
	}

	blockquote {
		background-color: #EEE;
		border-left: 2px dotted #FFF;
		border-right: 2px dotted #FFF;
		padding: 20px 30px 10px;

		p {
			font-family: $adelle;
			margin-bottom: 10px;
		}
	}

	@extend .txt-defaults;
}

.article-txt-reverse {
	@extend .article-txt;
	color: #ccc;
	p, ul, li, blockquote {
		font-weight: 300;
	}
}


/* ============================================= */
/*	Comments				   			 */ 
/* ============================================= */

.comments-mod {
	max-width: 500px;
	margin: 70px auto 0;
}

.comments {
	color: #000;
	// Hide pingbacks, they are annoying and unsightly! That's that shit we don't like!
	.pingback {
		display: none;
	}
}

.blog-comment {
	padding: 20px 0;
}

.comment-num {
	height: 30px;
	width: 30px;
	font: 400 14px $adelle;
	border: 1px solid rgb(204,204,204);
	text-align: center;
	padding: 4px 0;
	margin: auto;
	color: #666;
	margin-bottom: 7px;	
	@include border-radius(100px);
}

.comment-author {
	font: 600 16px/24px $adelle;
	color: $highlight;
	position:relative;
	text-align: center;
	margin-bottom: 20px;
}

.comment-content {
	max-width: 600px;
	margin: auto;
	p {
		font: 400 16px/26px $proxima;
		margin: 0 0 20px 0;
	}
	a {
		color: $link;
	}
}

.respond-h {
	text-align: center;
}

.respond {
	margin-bottom: 40px;
	color: #000;

	#reply-title {
		display: none;
	}

	p {
		padding: 10px 0;
		@include pie-clearfix;
	}

	label {
		display: inline-block;
		margin-bottom: 6px;
		font: 600 14px/18px $proxima;
	}

	input[type="text"], textarea {
		width: 100%;
		display: block;
		padding: 10px;
		border: 1px solid #CCC;
	}

	code {
		color: #666;
	}

	.form-allowed-tags {
		width: 75%;
		float: left;
		border: 0;

		@media (max-width: 600px){
			display: none;
		}
	}

	.form-submit {
		border: 0;
	}

	#submit {
		@extend .btn-large;
		display: block;
		margin: auto;
	}

	// This refers to the grouping of extra fields
	// in addition to the comment textarea
	#respond {
		display: none;
	}

	.logged-in-as {
		font-size: 11px;
		text-align: center;
		color: #666;
		margin-top: -10px;		
		> a {
			color: $highlight;
		}
	}
}

// Bring in extra styles that
// beautify individual blog posts 
@import "project/blog";



/* ============================================= */
/*	Portfolio detail 			 				 */ 
/* ============================================= */

// these are styles for how to display groups of images. 
.portfolio-layouts {

	@include pie-clearfix;

	.portfolio-image-set {
		max-width: 1050px;
		> img {
			margin: 50px 0;
		}
	}

	@media (max-width: 899px){
		.image-count-1, .image-count-2, .image-count-3 {
			img {
				width: 100%;
				float: left;
			}	
		}
	}

	@media (min-width: 900px){
		.image-count-1 {
			margin: 20px -150px;

			img{
				width: 100%;
				height: auto;

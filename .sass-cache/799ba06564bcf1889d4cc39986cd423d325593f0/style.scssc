//These are the areas beneath the main slider

.hp-theme {
	@include pie-clearfix;
	padding: 20px 0;
	border-bottom: 1px dotted #222;
	&:last-child {
		border: 0;
	}

	.txt-oversize {
		color: #666;
		font-weight: 300;
	}

	.hp-theme-intro {
		margin-bottom: 10px;
		padding-bottom: 10px;
	}

	.hp-theme-tile-grid {
		&.reverse {
			float: left;
		}
		// @include media-grid(3);

		img {
			width: 40%;
			float: left;
			margin: 0 10px 0 0;
			z-index: 10000;
		}

		.media-block {
			margin-bottom: 10px;
			display: block;
			@include pie-clearfix;

			.tile-h4 {
				position: absolute;
				left: 35%;
				width: 55%;
				padding: 22px 10px 22px 40px;
				background-color: rgba(0,0,0,.25);
				z-index: 9999;
			}

			&:hover {
				.tile-h4 {
					color: #666;
				}
			}
		}
	}//hp theme tiles 

	@media (min-width: 480px) and (max-width:959px) {
		.hp-theme-intro,
		.hp-theme-tiles {
			max-width: 500px;
			margin: auto;
		}
	} // mq

	@media (min-width: 480px) {
		.hp-theme-tile-grid {
			@include media-grid(3);

			img {
				width: 100%;
				clear: both;
				margin-bottom: 10px;
			}

			.media-block {
				background: none;

				.tile-h4 {
					padding: 0;
					position: static;
					width: 100%;
					background: none;
				}
			}
		}
	}

	@media (min-width: 960px){
		@include ui-grid-lr(50%, 50%, 6%);
		padding: 20px;
		.hp-theme-intro {
			border: 0;
			&.reverse {
				float: right;
			}
		}
	} // mq

}//hp-theme


// This is the section below the themes, right now with contact info
.hp-downpage {
	padding: 20px 10px;
	text-align: center;
	@media (min-width: 900px){
		padding: 30px 0 0 0;
		@include ui-grid-lr;
		margin: 0 0 30px 0;
	}
	.hp-form {
		max-width: 395px;
		margin: 10px auto;
	}
}

.hp-feeds {
	padding-top: 60px;
}

.hp-blog-list {
	//padding: 10px;

	@media (min-width: 600px) and (max-width:959px){
		@include media-grid(2,40px);
		padding: 0;
	}

	@media (min-width: 960px){
		@include media-grid(4,40px);
		padding: 0;
	}
}

.hp-tweets {
	@include pie-clearfix;
	padding: 30px 0;
	border-top: 1px solid #222;
	text-align: center;
	@media (min-width: 600px) {
		text-align: left;
	}
}

.tweet-handle {
	font: 700 16px $proxima;
	letter-spacing: 3px;
	text-transform: uppercase;
	color: #fff;

	@media(min-width: 600px){
		float: left;
		width: 200px;
	}
}

.tweet {
	margin-top: -1px;

	@media(min-width: 600px){
		margin-left: 220px;
	}
}

.tweet-txt {
	@extend .txt-oversize;
	> a {
		color: $highlight;
	}	 
}

.tweet-icon {
	display: block;
	height: 22px;
	width: 22px;
	margin: 0 auto 20px;
	@include opacity(.45);
	@include svg(twtr-wht);
	@media (min-width: 600px) {
		display: inline-block;
		margin: 0 9px 0 0;		
	}	
}

.tweet-handle-txt {
	display: none;
	position: relative;
	top: -5px;
	@media (min-width: 600px) {
		display: inline-block;
	}	
}



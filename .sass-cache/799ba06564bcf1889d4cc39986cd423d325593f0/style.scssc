/* ============================================= */
/*	Portfolio archive				   			 */ 
/* ============================================= */
.portfolio-featured-top {
	margin-bottom: 40px;
}

.portfolio-top-text {
	position: relative;

	.h1 {
		color: #FFF;
		margin: 10px 0;
	}

	@media(min-width:768px){
		margin-top: -100px;
		background-color: rgba(0,0,0,.75);
		padding: 20px;
		height: 100px;

		.h1 {
			width: 50%;
			float: left;
		}

		.txt {
			width: 50%;
			float: right;
		}
	}

	@include pie-clearfix;
}


#archive-portfolio-featured {
	@media(min-width:768px){
		@include media-grid(2, 40px);
	}
}

.portfolio-featured {
	@extend .media-block;
	margin-bottom: 40px;
}

.portfolio-featured-image {
	img {
		width: 100%;
		height: auto;
	}
}

.portfolio-featured-text {
	position: relative;

	@media(min-width:768px){
		margin-top: -100px;
		padding: 20px;
		height: 100px;
		background-color: rgba(0,0,0,.75);

	}

	.h2 {
		margin: 10px 0;
		color: #FFF;
	}
}


.portfolio-hgroup {
	margin: 30px 0;
	text-align: center;
}

.portfolio-h1 {
	margin-bottom: 10px;
}

.portfolio-meta {
	font: 300 17px $proxima;
	color: #333;
	a {
		color: $highlight;
	}
}

.archive {
	.ftr {
		margin-top: 0;
	}
}

#archive-clients {
	border-bottom: 0;
	padding-bottom: 80px;
}

.archive-clients-list {
	font: 500 14px $proxima;
	margin-bottom: 40px;

	li {
		color: #666;
		display: block;
		margin-bottom: 12px;
	}

	li a {
		color: #ccc;
		&:hover {
			color: $highlight;
		}
	}

	@media (max-width: 749px) {
